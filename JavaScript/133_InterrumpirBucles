console.log("Inicio\n")

for (let i = 1; i <= 10; i++){
    if (i == 5) {
        continue; //Al tener el continue no se imprime el 5
    }
    console.log(i);
}

for (let i = 1; i <= 10; i++){
    if (i == 5) {
        break; //Aqui se interrumpe porque se cumple la condicion
    }
    console.log(i);
}
console.log("\nFin")