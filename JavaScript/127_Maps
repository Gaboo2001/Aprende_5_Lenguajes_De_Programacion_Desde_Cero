console.log("Inicio\n")

const alumnos = new Map ([["Gabo",10],["Tokio", 9],["Lisboa",8]]);

alumnos.set("Rio",7);
console.log(alumnos);

console.log(alumnos.get("Gabo"));

alumnos.delete("Tokio");
alumnos.delete("Resultado despues de eliminar tokio con delete");
console.log(alumnos);

console.log(alumnos.has("Gabo"));

console.log(alumnos.size);
console.log("\nFin")