
#include <stdio.h>
#include <stdlib.h>

int main()
{

   printf ("%f", 2.5 + 3);
   printf ("\n%.1f", 2.5 + 3);
   printf ("\n%f",pow (2,5));
   printf ("\n%f",sqrt (81));
   printf ("\n%f",round (93.441));
   printf ("\n%f",ceil(93.441)); //redondea hacia arriba
   printf ("\n%f",floor(93.441)); //redondea hacia abajo


    return 0;

}
